<Window x:Class="SingleProfitTester.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Loaded="Window_Loaded_1"
        Title="Single Sequence Tester" Height="600" Width="525">
    <Grid>
        <StackPanel Margin="10">
            <TextBlock Margin="5">Enter Sequence</TextBlock>
            <StackPanel Orientation="Horizontal">
                <TextBox  Margin="5" Width="250" HorizontalAlignment="Left" Name="input"></TextBox>
                <Button Margin="5" Width="50" Click="Button_Click_1">GO!</Button>
                <Button Margin="5" Width="50" Click="Button_Click_2">Clear!</Button>
            </StackPanel>
            <ScrollViewer  VerticalScrollBarVisibility="Auto" Background="LightBlue" Height="500">
                <TextBlock  Name="outputBox" TextWrapping="Wrap"  ></TextBlock>
            </ScrollViewer>
            
        </StackPanel>
    </Grid>
</Window>
