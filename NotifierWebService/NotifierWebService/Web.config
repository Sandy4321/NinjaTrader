		<add name="AlsiDbConnectionString1" connectionString="Data Source=sql.alsitm.com,1444;Initial Catalog=AlsiDb;Persist Security Info=True;User ID=Pieter;Password=1Rachelle" providerName="System.Data.SqlClient"/>
